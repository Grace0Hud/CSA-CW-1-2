import java.lang.Math;

public class myMath
{
    //prints out a random number from 0 to 10
    public static void main(String args[]) 
    {
      int x=11;//must be above the number you actually want
      double y = Math.random();
      double z = (int) (x * y);//doesn't round just cuts of the decimal 
      
      System.out.println("Random number between 0 and 10: " + z);;
    }//end of main
}//end of myMath
